<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AllTestsSuite">
    <test name="Testing myds Test">
        <classes>
            <class name="io.github.battlerhythm.StackTest">
                <methods>
                    <include name="testToString" />
                    <include name="testIsEmpty" />
                    <include name="testPush" />
                    <include name="testPeek" />
                    <include name="testPop" />
                </methods>
            </class>
            <class name="io.github.battlerhythm.QueueTest">
                <methods>
                    <include name="testToString" />
                    <include name="testSize" />
                    <include name="testIsEmpty" />
                    <include name="testEnqueue" />
                    <include name="testDequeue" />
                </methods>
            </class>
            <class name="io.github.battlerhythm.DequeTest">
                <methods>
                    <include name="testEnqueueLeft" />
                    <include name="testDequeueRight" />
                </methods>
            </class>
            <class name="io.github.battlerhythm.NodeTest">
                <methods>
                    <include name="testGetItem" />
                    <include name="testSetItem" />
                    <include name="testGetNode" />
                    <include name="testSetNode" />
                </methods>
            </class>
        </classes>
    </test>
</suite>